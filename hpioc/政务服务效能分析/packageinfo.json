{
	"packagInfo": {
		"name": "data_stat",
		"version": "1.0",
		"init": "modelRun.init",
		"SQLdata": "modelRun.SQLdata"
	},
	"steps": {
		"init": [
			{
				"parameterName": "whlsource",
				"parameterType": "String",
				"defaultValue": "${prop.sysurl}/whl",
				"desc": "依赖包下载地址"
			}
		],
		"SQLdata": [

           {
				"parameterName": "mysqlurl",
				"parameterType": "String",
				"defaultValue": "",
				"desc": "读取库mysql库连接信息，格式为jdbc:mysql://ip/dbname"
			},
			{
				"parameterName": "username",
				"parameterType": "String",
				"defaultValue": "",
				"desc": "mysql连接信息，用户名"
			},
			{
				"parameterName": "password",
				"parameterType": "String",
				"defaultValue": "",
				"desc": "mysql连接信息，密码"
			},
			{
				"parameterName": "tablename",
				"parameterType": "String",
				"defaultValue": "",
				"desc": "查询数据表名,格式为模式名.表名"
			},
			{
				"parameterName": "tablename1",
				"parameterType": "String",
				"defaultValue": "",
				"desc": "查询数据表名,格式为模式名.表名"
			},
			{
				"parameterName": "tablename2",
				"parameterType": "String",
				"defaultValue": "",
				"desc": "查询数据表名,格式为模式名.表名"
			},
			{
				"parameterName": "wtable",
				"parameterType": "String",
				"defaultValue": "",
				"desc": "插入数据表名,格式为模式名.表名"
			}
		]
	}
}