{
	"packagInfo": {
		"name": "data_extract",
		"version": "1.0",
		"init": "modelRun.init",
		"SQLdata": "modelRun.SQLdata"
	},
	"steps": {
		"init": [
			{
				"parameterName": "whlsource",
				"parameterType": "String",
				"defaultValue": "${prop.whlurl}",
				"desc": "依赖包下载地址"
			}
		],
		"SQLdata": [

           {
				"parameterName": "url_rkjbxx",
				"parameterType": "String",
				"defaultValue": "",
				"desc": "读取库mpp连接信息1，人口基本信息表库，格式为jdbc:postgresql://ip/dbname"
			},
			{
				"parameterName": "url_oldpeopleneedmonthnew",
				"parameterType": "String",
				"defaultValue": "",
				"desc": "读取库mpp连接信息2，oldpeopleneedmonthnew表库，格式为jdbc:postgresql://ip/dbname"
			},
			{
				"parameterName": "url_niunai_zhucan_ku",
				"parameterType": "String",
				"defaultValue": "",
				"desc": "写入库mpp连接信息，助餐牛奶库，格式为jdbc:postgresql://ip/dbname"
			},
			{
				"parameterName": "username",
				"parameterType": "String",
				"defaultValue": "",
				"desc": "mpp连接信息，用户名"
			},
			{
				"parameterName": "password",
				"parameterType": "String",
				"defaultValue": "",
				"desc": "mpp连接信息，密码"
			},
			{
				"parameterName": "tablename_rkjbxx",
				"parameterType": "String",
				"defaultValue": "",
				"desc": "查询数据表名1,rkjbxx，格式为模式名.表名"
			},
			{
				"parameterName": "tablename_oldpeopleneedmonthnew",
				"parameterType": "String",
				"defaultValue": "",
				"desc": "查询数据表名2,oldpeopleneedmonthnew，格式为模式名.表名"
			},
			{
				"parameterName": "wtable_niunai_degree",
				"parameterType": "String",
				"defaultValue": "",
				"desc": "插入数据表名1,牛奶需求,格式为模式名.表名"
			},
			{
				"parameterName": "wtable_niunai_qushi",
				"parameterType": "String",
				"defaultValue": "",
				"desc": "插入数据表名2,牛奶趋势,格式为模式名.表名"
			},
			{
				"parameterName": "wtable_niunai_top5",
				"parameterType": "String",
				"defaultValue": "",
				"desc": "插入数据表名3,牛奶top5,格式为模式名.表名"
			},
			{
				"parameterName": "wtable_zhucan_degree",
				"parameterType": "String",
				"defaultValue": "",
				"desc": "插入数据表名4,助餐需求,格式为模式名.表名"
			},
			{
				"parameterName": "wtable_zhucan_qushi",
				"parameterType": "String",
				"defaultValue": "",
				"desc": "插入数据表名5,助餐趋势,格式为模式名.表名"
			},
			{
				"parameterName": "wtable_zhucan_top5",
				"parameterType": "String",
				"defaultValue": "",
				"desc": "插入数据表名6,助餐top5,格式为模式名.表名"
			},
			{
				"parameterName": "wtable_niunai_total",
				"parameterType": "String",
				"defaultValue": "",
				"desc": "插入数据表名7,牛奶总量,格式为模式名.表名"
			},
			{
				"parameterName": "wtable_zhucan_total",
				"parameterType": "String",
				"defaultValue": "",
				"desc": "插入数据表名8,助餐总量,格式为模式名.表名"
			}
		]
	}
}