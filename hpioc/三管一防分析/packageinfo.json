{
	"packagInfo": {
		"name": "三管一防全量(新库)",
		"version": "1.0",
		"init": "modelRun.init",
		"SQLdata": "modelRun.SQLdata"
	},
	"steps": {
		"init": [
			{
				"parameterName": "whlsource",
				"parameterType": "String",
				"defaultValue": "${prop.whlurl}",
				"desc": "依赖包下载地址"
			}
		],
		"SQLdata": [

           {
				"parameterName": "url",
				"parameterType": "String",
				"defaultValue": "",
				"desc": "读取库mpp连接信息，格式为jdbc:postgresql://ip/dbname"
			},
			{
				"parameterName": "url1",
				"parameterType": "String",
				"defaultValue": "",
				"desc": "写入库mpp连接信息，格式为jdbc:postgresql://ip/dbname"
			},
			{
				"parameterName": "username",
				"parameterType": "String",
				"defaultValue": "",
				"desc": "mpp连接信息，用户名"
			},
			{
				"parameterName": "password",
				"parameterType": "String",
				"defaultValue": "",
				"desc": "mpp连接信息，密码"
			},
			{
				"parameterName": "tablename",
				"parameterType": "String",
				"defaultValue": "",
				"desc": "查询数据表名,格式为模式名.表名"
			},
			{
				"parameterName": "tablename_wg",
				"parameterType": "String",
				"defaultValue": "",
				"desc": "查询数据表名,格式为模式名.表名"
			},
			{
				"parameterName": "wtable",
				"parameterType": "String",
				"defaultValue": "",
				"desc": "插入数据表名,格式为模式名.表名"
			}
		]
	}
}